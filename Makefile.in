NAME=atlas
LIBNAME=lib

PLUGINS=

include rules.mk

DYNAMO_ARGS=db uml/atlas.zargo

post-build:
ifneq (${PLUGINS},)
	for i in ${PLUGINS} ; do \
	   $(MAKE) -C plugins/$$i ; \
	done
endif

post-generate:
ifneq (${PLUGINS},)
	for i in ${PLUGINS} ; do \
	   $(MAKE) -C plugins/$$i generate ; \
	done
endif
